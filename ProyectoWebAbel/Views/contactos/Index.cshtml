@model IEnumerable<ProyectoWebAbel.contactos>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.apellido)
        </th>
        <th>
            <label class="control-label col-md-2">Cedula/Pasaporte</label>
            @*@Html.DisplayNameFor(model => model.identidad)*@
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha_creacion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.apellido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.identidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fecha_creacion)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.contactoId }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.contactoId }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.contactoId })
        </td>
    </tr>
}

</table>
